<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.mapper.StudentMapper">

    <select id="getStudentListBycid" parameterType="int" resultType="com.qf.pojo.Student">
        select * from student where cid=#{cid}
    </select>
    <select id="getStudentByUid" parameterType="int" resultType="com.qf.pojo.Student">
        select * from student where uid=#{uid}
    </select>
</mapper>